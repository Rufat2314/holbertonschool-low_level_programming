#include <stdio.h>

/**
 * main - Prints the alphabetic in lower and upper case
 *
 * Return: Always (Success)
 */
int main(void)
{
        int a;

        for (c = 'a'; c <= 'z'; c++)
        {
                putchar(c);
        }

        
        putchar('\n');

        return (0);
}
